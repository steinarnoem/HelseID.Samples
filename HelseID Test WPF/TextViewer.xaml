<Window x:Class="HelseID.Test.WPF.TextViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Text viewer" Height="300" Width="300" Loaded="Window_Loaded" WindowStartupLocation="CenterOwner">
    <Grid>
        <TextBox x:Name="textBox" Margin="10" TextWrapping="Wrap" Text="TextBox" VerticalScrollBarVisibility="Auto"/>
    </Grid>
</Window>
